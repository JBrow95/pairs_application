<head>
	<link rel="stylesheet" type="text/css" href="css/random.css">
	<script src="js/style.js"></script>
	<title>JB | Random Pairs</title>
</head>


<body>
	
	<div class="header">
		<h1>Random Pairs</h1>
	</div>

	<div class="boxForm">
		<form method="post" action="/pair" id="myForm">
			<div id="preBox"></div>
			<div id="box"></div><br>
			<button type="button" onclick="addBox()">+</button>
			<button type="button" onclick="removeBox()">Remove</button>
			<button type="button" onclick="clearFunc()">Clear</button>
			<button>Pair</button>
			<p><%= names%></p>
		</form>
	</div>
</body>